#如果用jdbc连接，就使用代理25003端口
listen petabase_jdbc :25003
mode tcp
option tcplog
balance leastconn

server bigdata6_jdbc 172.21.10.16:21050
server bigdata7_jdbc 172.21.10.17:21050
server bigdata8_jdbc 172.21.10.18:21050
server bigdata9_jdbc 172.21.10.19:21050
server bigdata10_jdbc 172.21.10.20:21050

#如果用petabase-shell连接，则使用25004端口，在petabase-shell里面执行 connect proxy_server:25004
listen petabase_shell :25004
mode tcp
option tcplog
balance leastconn

server bigdata6_shell 172.21.10.16:21000
server bigdata7_shell 172.21.10.17:21000
server bigdata8_shell 172.21.10.18:21000
server bigdata9_shell 172.21.10.19:21000
server bigdata10_shell 172.21.10.20:21000
